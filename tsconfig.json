{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "moduleSuffixes": [".ios", ".android", ".native", ""]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expect-matchers.d.ts",
    "nativewind-env.d.ts"
  ]
}
